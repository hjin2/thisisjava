package sec04.exam01_unsynchronized;

public class MainThreadExample {
public static void main(String[] args) {
	Calculator calculator = new Calculator();
	
	User1 user1 = new User1();
	user1.setCalculator(calculator);
	user1.start();
	
	User1 user1 = new User1();
	user1.setCalculator(calculator);
	user1.start();
}
}
